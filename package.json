{
  "name": "tailwind-compositor-starter-static",
  "private": true,
  "description": "Tailwind compositor static starter",
  "author": "Apostolos Christodoulou <a7sc11u@gmail.com> (@a7sc11u)",
  "license": "MIT",
  "keywords": [
    "tailwindcss",
    "typography",
    "baseline",
    "baseline-grid",
    "baseline-typography",
    "grid-system",
    "vertical-rhythm"
  ],
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "fontkit": "^1.8.1",
    "parcel": "^1.12.4",
    "postcss": "^7.0.30",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "stylelint": "^13.5.0",
    "stylelint-config-standard": "^20.0.0",
    "tailwindcss": "^1.4.6"
  },
  "browserslist": "> 1%, not ie 11, not op_mini all",
  "scripts": {
    "develop": "rm -rf .cache && parcel src/index.html -d dev --target browser",
    "build": "rm -rf .cache && parcel src/index.html dist --target browser"
  },
  "dependencies": {
    "lodash.flattendeep": "^4.4.0",
    "tailwind-compositor": "^0.0.1-rc.12"
